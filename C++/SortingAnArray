
#include <iostream>
using namespace std;


void sortinator3000(int array[],int k)
{
	int min,temp;

	for (int i = 0; i < k - 1; i++)
	{
		min = i;

		for (int l = 0; l < k;l++)
		{
			if (array[min] > array[l])
			{
			
			}


		}

		cout << min << " << ";

	}

	cout << "The smallest number is: " << min;
}


int main()
{
	int randomNumbers[] = { 1,-8,9,10,12,91,-4,-13,1 };

	int sizeOfArray = sizeof(randomNumbers) / sizeof(int);

	cout << "The size of the array is: "<< sizeOfArray << endl;

	for (int i = 0; i < sizeOfArray; i++)
	{
		cout << randomNumbers[i] << "   ";
	}
	cout << endl;

	sortinator3000(randomNumbers, sizeOfArray);



}
