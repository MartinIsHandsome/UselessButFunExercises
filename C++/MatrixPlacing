#include <iostream>
using namespace std;


void createField(int playerX, int playerY)
{

	int sizeOfField[9][9];
	int Collum = 0;
	for (int i = 0; i < 9; i++)
	{

		while(Collum!=9){
		sizeOfField[i][Collum] = i;
		cout << "{ " << "}";
		}
	}
	

}


int main()
{

	 int sizePlaceX,sizePlaceY;
	 
