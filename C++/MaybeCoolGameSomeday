void createField(int playerX, int playerY)
{

	int sizeOfField[9][9];
	int Collum = 0;
	for (int i = 0; i < 9; i++)
	{

		for (int i1 = 0; i1 < 9; i1++) {
			sizeOfField[i][i1] = i;
			cout << "{ " << sizeOfField[i][i1] << "}";
		}
	}

	

}


