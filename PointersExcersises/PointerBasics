// ConsoleApplication3.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
using namespace std;

int main()
{
	int rawr[] = { 1,5,6,9,90,109291 };

	int sizeofInator = sizeof(rawr) / sizeof(rawr[0]);


	cout << sizeofInator;
	
	
	int*pRawr = rawr;

	for (int i = 0; i < sizeofInator; i++)
	{
		cout << *(pRawr + i) << " ";
	}

	//This time with an Array as well, improvement people!
}
