
#include <iostream>

using namespace std;

int main()
{
   int enterNumber,enterExtend;
   cin >> enterNumber >> enterExtend;
   int originalNum = enterNumber;
   for(int i=0; i < enterExtend; i++)
   {
       
       cout << "The number is:" << enterNumber << endl;
       enterNumber = enterNumber* originalNum;
   }
   
}
